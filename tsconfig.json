{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@tn/libs": [
        "./packages/libs"
      ],
      "@tn/components": [
        "./components"
      ],
      "@tn/atoms": [
        "./components/atoms"
      ],
      "@tn/molecules": [
        "./components/molecules"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
